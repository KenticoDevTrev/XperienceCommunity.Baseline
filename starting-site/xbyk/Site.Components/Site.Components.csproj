<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <WarningsAsErrors>Nullable</WarningsAsErrors>
    <AssemblyName>$(MSBuildProjectName)</AssemblyName>
    <RootNamespace>Site</RootNamespace>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="XperienceCommunity.Baseline.Account.RCL" Version="2.5.1" />
    <PackageReference Include="XperienceCommunity.Baseline.Core.RCL" Version="2.5.1" />
    <PackageReference Include="XperienceCommunity.Baseline.Localization.RCL" Version="2.5.1" />
    <PackageReference Include="XperienceCommunity.Baseline.Navigation.RCL" Version="2.5.1" />
    <PackageReference Include="XperienceCommunity.Baseline.Search.RCL" Version="2.5.1" />
    <PackageReference Include="XperienceCommunity.Baseline.TabbedPages.RCL" Version="2.5.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Site.Library\Site.Library.csproj" />
  </ItemGroup>



</Project>
