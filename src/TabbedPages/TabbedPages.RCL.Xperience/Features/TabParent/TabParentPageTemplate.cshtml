@model ComponentViewModel<TabbedPages.Features.TabParent.TabParentPageTemplateProperties>
@inject IPageIdentityFactory pageIdentityFactory
@inject IPageContextRepository pageContextRepository

@{
    // Convert to identity with our TabParentItem model
    if ((await pageContextRepository.GetCurrentPageAsync()).TryGetValue(out var identity)) {
        <vc:tab-parent x-page="@identity" />        
    }
}
